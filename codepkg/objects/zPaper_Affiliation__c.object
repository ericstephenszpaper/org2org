    <webLinks>
        <fullName>Mass_Send_Zippi</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mass Send Zippi</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var records = {!GETRECORDIDS($ObjectType.zPaper_Affiliation__c)};
if (records[0] == null) {
alert(&quot;Please select at least one row&quot;)
} else {
var myForm = document.createElement(&quot;form&quot;);
myForm.setAttribute(&quot;method&quot;, &quot;GET&quot;);
myForm.setAttribute(&quot;action&quot;, &quot;/apex/Mass_Send_Zippi&quot;); 

var sfIds = records[0].trim();
for(var i = 1; i &lt; records.length; i++) {
sfIds += &quot;,&quot; + records[i].trim();
}

myInput = document.createElement(&quot;input&quot;) ;
myInput.setAttribute(&quot;name&quot;, &quot;sfIds&quot;);
myInput.setAttribute(&quot;value&quot;, sfIds);
myForm.appendChild(myInput);
document.body.appendChild(myForm);

if (
confirm(&quot;Send &quot; + records.length + &quot; faxes?&quot;)) 
myForm.submit() ;
document.body.removeChild(myForm) ;
}

