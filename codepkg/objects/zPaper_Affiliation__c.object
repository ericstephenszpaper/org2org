    <webLinks>
        <fullName>massSend</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mass Send</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var records = {!GETRECORDIDS($ObjectType.zPaper_Affiliation__c)}; 
if (records[0] == null) { 
alert(&quot;Please select at least one row&quot;) 
} else { 
var myForm = document.createElement(&quot;form&quot;) 
myForm.setAttribute(&quot;method&quot;, &quot;POST&quot;); 
myForm.setAttribute(&quot;action&quot;, &quot;https://{!$Setup.ZPAPER__zpaper__c.ZPAPER__server__c}/kb/jsp/massFaxBin.jsp&quot;); 

var myInput = document.createElement(&quot;input&quot;) ; 
myInput.setAttribute(&quot;name&quot;, &quot;sfUser&quot;) ; 
myInput.setAttribute(&quot;value&quot;,&quot;{!$User.Username}&quot;); 
myForm.appendChild(myInput); 

myInput = document.createElement(&quot;input&quot;) ; 
myInput.setAttribute(&quot;name&quot;, &quot;broadcast&quot;) ; 
myInput.setAttribute(&quot;value&quot;,&quot;1&quot;); 
myForm.appendChild(myInput); 

myInput = document.createElement(&quot;input&quot;) ; 
myInput.setAttribute(&quot;name&quot;, &quot;SFserver&quot;) ; 
myInput.setAttribute(&quot;value&quot;,&quot;{!$Api.Partner_Server_URL_160}&quot;); 
myForm.appendChild(myInput); 

myInput = document.createElement(&quot;input&quot;) ; 
myInput.setAttribute(&quot;name&quot;, &quot;SFsession&quot;) ; 
myInput.setAttribute(&quot;value&quot;,&quot;{!$Api.Session_ID}&quot;); 
myForm.appendChild(myInput); 

myInput = document.createElement(&quot;input&quot;) ; 
myInput.setAttribute(&quot;name&quot;, &quot;sfOrg&quot;) ; 
myInput.setAttribute(&quot;value&quot;,&quot;{!$Organization.Id}&quot;); 
myForm.appendChild(myInput); 

myInput = document.createElement(&quot;input&quot;) ; 
myInput.setAttribute(&quot;name&quot;, &quot;SFtype&quot;) ; 
myInput.setAttribute(&quot;value&quot;,&quot;zPaper_Affiliation__c&quot;); 
myForm.appendChild(myInput); 

myInput = document.createElement(&quot;input&quot;) ; 
myInput.setAttribute(&quot;name&quot;, &quot;sfIDs&quot;) ; 
myInput.setAttribute(&quot;value&quot;,records.toString()); 
//alert(records.toString()); 
myForm.appendChild(myInput); 

myInput = document.createElement(&quot;input&quot;) ; 
myInput.setAttribute(&quot;name&quot;, &quot;sendToFax&quot;) ; 
myInput.setAttribute(&quot;value&quot;,&quot;{!zPaper_Affiliation__c.Fax__c}&quot;); 
myForm.appendChild(myInput); 

document.body.appendChild(myForm) ; 
if (confirm(&quot;Send &quot; + records.length + &quot; faxes?&quot;)) myForm.submit() ; 
document.body.removeChild(myForm) ; 
}</url>
