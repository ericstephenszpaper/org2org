<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <fields>
        <fullName>Received_Date_Time__c</fullName>
        <description>This is replacing the Received At from the Managed Package</description>
        <externalId>false</externalId>
        <formula>CreatedDate</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Received Date/Time</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Time_Accepted_Date__c</fullName>
        <description>This is the time and date a zStack was accepted from the Main Intake List</description>
        <externalId>false</externalId>
        <label>Time Accepted Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Time_Complete_Date__c</fullName>
        <description>Our test for 2</description>
        <externalId>false</externalId>
        <label>Time Complete Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>zStack_Accepted_Age_in_Minutes__c</fullName>
        <description>Time when a zStack was received and then accepted in minutes</description>
        <externalId>false</externalId>
        <formula>(Time_Accepted_Date__c -   Received_Date_Time__c  ) *1440</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Accepted Time</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>zStack_Completed_Age_in_Minutes__c</fullName>
        <description>This is the formula that calculates the time from when a zStack is accepted to when it is completed</description>
        <externalId>false</externalId>
        <formula>( Time_Complete_Date__c - Time_Accepted_Date__c) * 1440</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Completed Time</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
</CustomObject>
