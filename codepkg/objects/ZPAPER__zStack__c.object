        <fullName>zStack_Accepted_Age_in_Minutes__c</fullName>
        <description>Time when a zStack was received and then accepted in minutes</description>
        <externalId>false</externalId>
        <formula>(Time_Accepted_Date__c -   Received_Date_Time__c  ) *1440</formula>
